<Application x:Class="RustSkinsEditor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RustSkinsEditor" xmlns:Converters="clr-namespace:RustSkinsEditor.Converters"
             StartupUri="MainWindow.xaml" DispatcherUnhandledException="App_DispatcherUnhandledException">
    <Application.Resources>
        <Converters:UriFromShortnameConverter x:Key="UriFromShortnameConverter"/>
        <Converters:IsMarketSkinConverter x:Key="IsMarketSkinConverter"/>
        <Converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <Converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>

    </Application.Resources>
</Application>
